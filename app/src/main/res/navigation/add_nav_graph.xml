<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/add_navigation"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.collegescheduler.ui.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_add_Course2"
            app:destination="@id/add_Course2" />
    </fragment>
    <fragment
        android:id="@+id/add_Course2"
        android:name="com.example.collegescheduler.ui.addCourse.Add_Course"
        android:label="fragment_add__course"
        tools:layout="@layout/fragment_add__course" >
        <action
            android:id="@+id/action_add_Course2_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
</navigation>